{
  "functions": {
    "api/admin.ts": {
      "runtime": "@vercel/node"
    },
    "api/lottery.ts": {
      "runtime": "@vercel/node"
    },
    "api/participants.ts": {
      "runtime": "@vercel/node"
    },
    "api/test.ts": {
      "runtime": "@vercel/node"
    }
  },
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/.*).*)",
      "destination": "/client/dist/index.html"
    }
  ]
}